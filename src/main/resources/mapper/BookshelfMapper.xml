<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.groupfour.Library.dao.BookshelfMapper">
    <insert id="insertBookshelf" parameterType="Map">
        insert into bookshelf (bookId,userId,bookName,author,addtime)
        values (#{bookId},#{userId},#{bookName},#{author},#{addtime});
    </insert>

</mapper>