<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.groupfour.Library.dao.SubscriptionMapper">
    <insert id="insertSubscription" parameterType="Map">
        insert into subscription (bookId,userId,userName,bookName,subtime)
        values (#{bookId},#{userId},#{userName},#{bookName},#{addtime});
    </insert>
</mapper>